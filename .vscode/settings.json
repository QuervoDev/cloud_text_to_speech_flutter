{
  "dart.lineLength": 120,
  "files.exclude": {
    "**/*.chopper.dart": true,
    "**/*.freezed.dart": true,
    "**/*.gr.dart": true,
    "**/*.g.dart": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  }
}